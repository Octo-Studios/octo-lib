{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${mod_group}",
    "id": "${mod_id}",
    "version": "${mod_version}",
    "metadata": {
      "name": "${mod_id}",
      "description": "${mod_description}"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "it.hurts.octostudios.${mod_id}.quilt.OctoLibQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.21"
      }
    ]
  },
  "mixin": [
    "${mod_id}.mixins.json"
  ]
}
